{
    "manifest_version": 2,
    "default_locale": "en",  
    "name": "__MSG_extensionName__",
    "description": "__MSG_extensionDescription__",
    "version": "1.2.3",
    "applications": {
	"gecko": {
	    "id": "mail2cal-ai@yourdomain.local",
	    "strict_min_version": "115.0"
	}
    },
  "permissions": [
    "messagesRead",
    "menus",
    "storage",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  }
}
